<h1>Admin::Items#index</h1>
<p>Find me in app/views/admin/items/index.html.erb</p>
<%= link_to "新規登録_商品", new_admin_item_path %>

<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>商品Id</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <% @items.each do |item| %>
  <tbody>
    <tr>
      <td><%= item.id %></td>
      <td><%= link_to item.name, admin_item_path(item.id) %></td>
      <td><%= item.price %></td>
      <td><%= item.genre_id %></td>
      <td><%= item.is_on_sale %></td>
    </tr>
  </tbody>
  <% end %>
</table>