<h1>Admin::Customers#index</h1>
<p>Find me in app/views/admin/customers/index.html.erb</p>

<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>会員Id</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <% @customers.each do |customer| %>
  <tbody>
    <tr>
      <td><%= customer.id %></td>
      <td><%= link_to admin_customer_path(customer.id) do %>
           <%= customer.name %>
          <% end %>
      </td>
      <td><%= customer.email %></td>
      <td><%= customer.is_deleted_status %></td>
    </tr>
  </tbody>
  <% end %>
</table>
